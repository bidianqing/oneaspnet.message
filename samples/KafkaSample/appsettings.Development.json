{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "System": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "KafkaOptions": {
    "ProducerConfig": {
      "BootstrapServers": "kafka1-uat.longfor.com:9092,kafka2-uat.longfor.com:9092,kafka3-uat.longfor.com:9092"
    },
    "ConsumerConfig": {
      "BootstrapServers": "kafka1-uat.longfor.com:9092,kafka2-uat.longfor.com:9092,kafka3-uat.longfor.com:9092",
      "AutoOffsetReset": "Earliest",
      "EnableAutoCommit": true,
      "AutoCommitIntervalMs": 5000,
      "SessionTimeoutMs": 30000
    },
    "AdminClientConfig": {
      "BootstrapServers": "kafka1-uat.longfor.com:9092,kafka2-uat.longfor.com:9092,kafka3-uat.longfor.com:9092"
    },
    "CustomConfig": {
      "EnableNumOfAutoCommit": true,
      "NumOfAutoCommit": 5
    }
  }
}
